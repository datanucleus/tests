<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jdo SYSTEM "file:/javax/jdo/jdo.dtd">
<jdo>
    <package name="org.jpox.samples.many_one.unidir">
        <class name="HireCar" detachable="true">
            <field name="registrationId" primary-key="true"/>
            <field name="make"/>
            <field name="model"/>
        </class>

        <class name="CarRental" detachable="true">
            <field name="customerId" primary-key="true"/>
            <field name="hireCar">
                <join/>
            </field>
            <field name="startDate"/>
            <field name="endDate"/>
        </class>
    </package>
</jdo>