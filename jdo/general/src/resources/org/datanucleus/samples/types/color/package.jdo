<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jdo PUBLIC
    "-//Sun Microsystems, Inc.//DTD Java Data Objects Metadata 2.0//EN"
    "http://java.sun.com/dtd/jdo_2_0.dtd">
<jdo>
    <package name="org.datanucleus.samples.types.color">
        <class name="ColorHolder">
            <field name="colorA" persistence-modifier="persistent">
               <column name="color_red"/>
               <column name="color_green"/>
               <column name="color_blue"/>
               <column name="color_alpha"/>
            </field>
        </class>
    </package>
</jdo>