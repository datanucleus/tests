<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
    version="1.0">
    <description>JPA MetaData</description>
    <persistence-unit-metadata>
        <xml-mapping-metadata-complete></xml-mapping-metadata-complete>
    </persistence-unit-metadata>
    <package>org.jpox.samples.types.calendar</package>

    <entity class="org.jpox.samples.types.calendar.CalendarHolder">
        <attributes>
            <id name="id"/>
            <basic name="cal1"/>
            <basic name="cal2">
                <temporal>DATE</temporal>
            </basic>
        </attributes>
    </entity>

</entity-mappings>