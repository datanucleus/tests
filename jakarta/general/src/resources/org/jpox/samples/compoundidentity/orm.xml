<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
    version="1.0">
    <description>JPA MetaData for classes solely using MetaData</description>
    <persistence-unit-metadata>
        <xml-mapping-metadata-complete></xml-mapping-metadata-complete>
    </persistence-unit-metadata>
    <package>org.jpox.samples.compoundidentity</package>

    <entity class="org.jpox.samples.compoundidentity.CompoundHolder">
        <table name="JPA_XM_COMP_HOLDER"/>
        <id-class class="org.jpox.samples.compoundidentity.CompoundHolder$Id"/>
        <attributes>
            <id name="id">
                <generated-value strategy="TABLE"/>
            </id>
            <id name="name"/>
            <transient name="list1"/>
            <transient name="list2"/>
            <transient name="list3"/>
            <transient name="map1"/>
            <transient name="map2"/>
        </attributes>
    </entity>

    <entity class="org.jpox.samples.compoundidentity.CompoundSingleTarget">
        <table name="JPA_XM_COMP_SINGLE_TARGET"/>
        <id-class class="org.jpox.samples.compoundidentity.CompoundSingleTarget$Id"/>
        <attributes>
            <id name="id">
                <generated-value strategy="TABLE"/>
            </id>
            <id name="holder"/>
            <one-to-one name="holder"/>
        </attributes>
    </entity>

</entity-mappings>